{
    "name": "@daysmart/cdk-routing-split",
    "version": "0.1.0",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tsc -p tsconfig.app.json",
        "package": "npm run build && serverless package --stage sandbox",
        "deploy:sandbox": "npm run build && serverless deploy --stage sandbox --aws-profile sandbox --verbose",
        "test:sandbox": "sls export-env --stage sandbox --aws-profile sandbox && jest",
        "test:sandbox2": "sls export-env --stage sandbox2 --aws-profile sandbox && jest",
        "watch": "tsc -p tsconfig.app.json -w",
        "lint": "eslint **/*.ts --ignore-path .gitignore",
        "lint:fix": "npm run lint -- --fix",
        "lint:all": "npm run lint && npm run prettier:check",
        "lint:fixAll": "npm run lint:fix && npm run prettier:fix",
        "lint:staged": "lint-staged",
        "prettier": "prettier -c --ignore-path .prettierignore \"**\" --ignore-unknown",
        "prettier:fix": "prettier --write --ignore-path .prettierignore \"**\" --ignore-unknown"
    },
    "devDependencies": {
        "@types/jest": "^26.0.10",
        "@types/mssql": "^7.1.4",
        "@types/node": "10.17.27",
        "aws-cdk-lib": "2.9.0",
        "constructs": "^10.0.54",
        "eslint": "7.20.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-unused-imports": "1.1.0",
        "@typescript-eslint/eslint-plugin": "4.15.2",
        "@typescript-eslint/parser": "4.15.2",
        "jest": "^26.4.2",
        "prettier": "^2.5.1",
        "serverless": "2.27.1",
        "serverless-export-env": "^1.4.4",
        "serverless-newrelic-lambda-layers": "^1.1.5",
        "serverless-plugin-modularize": "1.0.9",
        "serverless-prune-plugin": "1.4.3",
        "ts-jest": "^26.2.0",
        "typescript": "4.1.5"
    },
    "peerDependencies": {
        "aws-cdk-lib": "2.9.0",
        "constructs": "^10.0.0"
    },
    "dependencies": {
        "@types/aws-lambda": "^8.10.92",
        "dsi-aws-boilerplate": "4.0.7",
        "serialize-error": "^8.1.0"
    }
}
