{
    "name": "@daysmart/cdk-routing-split",
    "version": "0.1.0",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tsc -p tsconfig.app.json",
        "watch": "tsc -p tsconfig.app.json -w",
        "lint": "eslint **/*.ts --ignore-path .gitignore",
        "lint:fix": "npm run lint -- --fix",
        "lint:all": "npm run lint && npm run prettier:check",
        "lint:fixAll": "npm run lint:fix && npm run prettier:fix",
        "lint:staged": "lint-staged",
        "prettier": "prettier -c --ignore-path .prettierignore \"**\" --ignore-unknown",
        "prettier:fix": "prettier --write --ignore-path .prettierignore \"**\" --ignore-unknown",
        "package": "npm run build && serverless package --stage sandbox",
        "deploy:sandbox": "npm run build && serverless deploy --stage sandbox --aws-profile sandbox --verbose",
        "test:sandbox": "sls export-env --stage sandbox --aws-profile sandbox && jest"
    },
    "devDependencies": {
        "@types/aws-lambda": "~8.10.93",
        "@types/chance": "^1.1.3",
        "@types/jest": "~27.4.1",
        "@types/mssql": "~7.1.5",
        "@types/node": "~17.0.21",
        "@typescript-eslint/eslint-plugin": "~5.15.0",
        "@typescript-eslint/parser": "~5.15.0",
        "aws-cdk-lib": "~2.16.0",
        "aws-sdk": "^2.1094.0",
        "chance": "^1.1.8",
        "constructs": "~10.0.89",
        "eslint": "~8.11.0",
        "eslint-plugin-import": "~2.25.4",
        "eslint-plugin-prefer-arrow": "~1.2.3",
        "eslint-plugin-unused-imports": "~2.0.0",
        "jest": "~27.5.1",
        "prettier": "~2.6.0",
        "serverless": "2.27.1",
        "serverless-export-env": "~2.1.0",
        "serverless-newrelic-lambda-layers": "~3.0.0",
        "serverless-plugin-modularize": "~2.0.0",
        "serverless-prune-plugin": "~2.0.1",
        "ts-jest": "~27.1.3",
        "typescript": "~4.6.2"
    },
    "peerDependencies": {
        "aws-cdk-lib": "~2.16.0",
        "constructs": "^10.0.0"
    },
    "dependencies": {
        "@daysmart/aws-lambda-logger": "~0.0.6",
        "tldts": "^5.7.69"
    }
}
