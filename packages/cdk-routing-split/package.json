{
    "name": "@daysmart/cdk-routing-split",
    "version": "0.1.0",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tsc -p tsconfig.app.json",
        "package": "npm run build && serverless package --stage sandbox",
        "deploy:sandbox": "npm run build && serverless deploy --stage sandbox --aws-profile sandbox --verbose",
        "test:sandbox": "sls export-env --stage sandbox --aws-profile sandbox && jest",
        "test:sandbox2": "sls export-env --stage sandbox2 --aws-profile sandbox && jest",
        "watch": "tsc -p tsconfig.app.json -w",
        "lint": "eslint **/*.ts --ignore-path .gitignore",
        "lint:fix": "npm run lint -- --fix",
        "lint:all": "npm run lint && npm run prettier:check",
        "lint:fixAll": "npm run lint:fix && npm run prettier:fix",
        "lint:staged": "lint-staged",
        "prettier": "prettier -c --ignore-path .prettierignore \"**\" --ignore-unknown",
        "prettier:fix": "prettier --write --ignore-path .prettierignore \"**\" --ignore-unknown"
    },
    "devDependencies": {
        "@types/aws-lambda": "~8.10.92",
        "@types/chance": "^1.1.3",
        "@types/jest": "~27.4.0",
        "@types/mssql": "~7.1.4",
        "@types/node": "~17.0.17",
        "@typescript-eslint/eslint-plugin": "~5.11.0",
        "@typescript-eslint/parser": "~5.11.0",
        "aws-cdk-lib": "~2.9.0",
        "aws-sdk": "^2.1078.0",
        "chance": "^1.1.8",
        "constructs": "~10.0.58",
        "eslint": "~7.20.0",
        "eslint-plugin-import": "~2.25.4",
        "eslint-plugin-prefer-arrow": "~1.2.3",
        "eslint-plugin-unused-imports": "~2.0.0",
        "jest": "~27.5.1",
        "prettier": "~2.5.1",
        "serverless": "2.27.1",
        "serverless-export-env": "~2.1.0",
        "serverless-newrelic-lambda-layers": "~2.4.0",
        "serverless-plugin-modularize": "~1.0.10",
        "serverless-prune-plugin": "~2.0.1",
        "ts-jest": "~27.1.3",
        "typescript": "~4.5.5"
    },
    "peerDependencies": {
        "aws-cdk-lib": "2.9.0",
        "constructs": "^10.0.0"
    },
    "dependencies": {
        "@daysmart/aws-lambda-logger": "~0.0.6"
    }
}
